#import <Foundation/Foundation.h>

@interface News : NSObject

//标题----
@property (nonatomic,strong)NSString    *title;
//区名称----
@property (nonatomic,strong)NSString    *sectionName;
//简短内容----
@property (nonatomic,strong)NSString    *summary;
//许可评论---
@property (nonatomic,strong)NSString    *permitComment;
//url---
@property (nonatomic,strong)NSString    *bannerUrl;
//阅读量---
@property (nonatomic,strong)NSString    *readCount;
//铅笔后面的字---
@property (nonatomic,strong)NSString    *plateName;
//创建时间---
@property (nonatomic,strong)NSString    *createDate;
//评论条数
@property (nonatomic,strong)NSString    *cmtCount;



//内容
@property (nonatomic,strong)NSString    *content;
//副标题
@property (nonatomic,strong)NSString    *subTitle;
//修改时间
@property (nonatomic,strong)NSString    *modifyDate;
//状态
@property (nonatomic,strong)NSString    *status;
//
@property (nonatomic,strong)NSString    *keyword;
//创建人员
@property (nonatomic,strong)NSString    *createStaffName;

@property (nonatomic,strong)NSString    *orderWeight;

@property (nonatomic,strong)NSString    *sourceType;

@property (nonatomic,strong)NSString    *newsID;

@property (nonatomic,strong)NSString    *newsPlateIdList;

@property (nonatomic,strong)NSString    *targetType;

@property (nonatomic,strong)NSString    *outSourceName;
//57策略
@property (nonatomic,strong)NSString    *section;

@property (nonatomic,strong)NSString    *outSourceUrl;
//新闻类型：0：纯文字 1：小图   2:大图
@property (nonatomic,strong)NSString    *picFlag;
//热门新闻：结束时间
@property (nonatomic,strong)NSString    *hotEndTime;
//是否热门
@property (nonatomic,strong)NSString    *hot;
//是否置顶
@property (nonatomic,strong)NSString    *top;


//新闻被读取的状态 0未读 1已读
@property (nonatomic,strong)NSString    *readStatus;
@end
